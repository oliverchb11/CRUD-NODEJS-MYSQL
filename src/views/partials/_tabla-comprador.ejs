<div class="col-md-7">
  <table class="table table-bordered table-hover">
    <thead class="bg-dark" id="tabla-cabezera">
      <tr>
        <th>
          id
        </th>
        <th>
          Nombre
        </th>
        <th>
          Direccion
        </th>
        <th>
          Telefono
        </th>
        <th>
          Accion
        </th>
      </tr>
    </thead>
    <tbody>
      <% if(data){ %> <% for(var i = 0; i < data.length;i++){ %>
      <tr>
        <td><%= i + 1 %></td>
        <td><%= data[i].nombre %></td>
        <td><%= data[i].direccion %></td>
        <td><%= data[i].telefono %></td>
        <td>
          <a href="/delete/<%= data[i].id %> " class="btn btn-danger">Borrar</a>
          <a href="/editar/<%= data[i].id %> " class="btn btn-warning"
            >Editar
          </a>
        </td>
      </tr>
      <% } %> <% } %>
    </tbody>
  </table>
</div>
